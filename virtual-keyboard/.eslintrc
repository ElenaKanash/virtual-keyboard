{
  "extends": ["airbnb-base"],

  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "import/extensions": ["error", "always"],
    "allowForLoopAfterthoughts": true,
    "func-names": "off"
  }

}


